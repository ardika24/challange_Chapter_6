<body>       
          <div class="d-flex justify-content-between">
            <h1 class=""><%= resourceName %></h1>
            <a href="/users/add" class="btn btn-lg btn-primary">Add User</a>
          </div>
          <div class="table-responsive">
            <table class="table table-hover table-striped">
              <thead>
                <tr>
                  <!-- <th scope="col">#</th> -->
                  <th scope="col">Username</th>
                  <th scope="col">Password</th>
                  <th scope="col">Status</th>
                  <th scope="col">Action</th>
                </tr>
              </thead>
              <tbody>
                <% users.forEach(el => { %>
                <tr>
                  <!-- <th scope="row">1</th> -->
                  <td><%= el.username %></td>
                  <td><%= el.password %></td>
                  <td><%= el.status %></td>
                  <td>
                    <a
                      href="/users/<%= el.id %>/edit"
                      class="btn btn-warning"
                      >Edit</a
                    >
                    <form
                      method="POST"
                      action="/users/<%= el.id %>?_method=DELETE"
                      class="d-inline"
                    >
                      <button type="submit" class="btn btn-danger">
                        Delete
                      </button>
                    </form>
                    <a
                      href="/users/<%= el.id %>/bio"
                      class="btn btn-success"
                      >Show bio</a
                    >
                    <a
                      href="/users/<%= el.id %>/histories"
                      class="btn btn-success"
                      >Show histories</a
                    >
                  </td>
                </tr>
                <% }) %>
              </tbody>
            </table>
          </div>